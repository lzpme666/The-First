#include <iostream>
#include <math.h>
using namespace std;
void main()
{
	int a[10]={0};

	int c=0;
	int n;
	cin >> n;
	for(i=9;i>=0;i--){
		c=n/(int)pow(10.0,i);
		a[0]-=(int)pow(10.0,i);
		for(int k=0;k<10;k++){
			if(k<c)
				a[k]+=(int)(pow(10.0,i)*(c/(int)pow(10.0,i)+1));
			else if(k=c)
				a[k]+=(int)(n/pow(10.0,i+1)*pow(10.0,i)+n%(int)pow(10.0,i)+1);
			else
				a[k]+=(int)(n/pow(10.0,i+1)*pow(10.0,i));
		}
	}
	for(i=0;i<10;i++){
		cout << a[i];
	}
}
